<?xml version="1.0" ?>
<form xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
	<argument name="data" xsi:type="array">
		<item name="js_config" xsi:type="array">
			<item name="provider" xsi:type="string">printq_storelocationsmanagement_storelocations_form.storelocations_form_data_source</item>
		</item>
		<item name="label" xsi:type="string" translate="true">General Information</item>
		<item name="template" xsi:type="string">templates/form/collapsible</item>
	</argument>
	<settings>
		<buttons>
			<button name="back" class="Printq\StoreLocationsManagement\Block\Adminhtml\StoreLocations\Edit\BackButton"/>
			<button name="delete" class="Printq\StoreLocationsManagement\Block\Adminhtml\StoreLocations\Edit\DeleteButton"/>
			<button name="save" class="Printq\StoreLocationsManagement\Block\Adminhtml\StoreLocations\Edit\SaveButton"/>
			<button name="save_and_continue" class="Printq\StoreLocationsManagement\Block\Adminhtml\StoreLocations\Edit\SaveAndContinueButton"/>
		</buttons>
		<namespace>printq_storelocationsmanagement_storelocations_form</namespace>
		<dataScope>data</dataScope>
		<deps>
			<dep>printq_storelocationsmanagement_storelocations_form.storelocations_form_data_source</dep>
		</deps>
	</settings>
	<dataSource name="storelocations_form_data_source">
		<argument name="data" xsi:type="array">
			<item name="js_config" xsi:type="array">
				<item name="component" xsi:type="string">Magento_Ui/js/form/provider</item>
			</item>
		</argument>
		<settings>
			<submitUrl path="*/*/save"/>
		</settings>
		<dataProvider name="storelocations_form_data_source" class="Printq\StoreLocationsManagement\Model\StoreLocations\DataProvider">
			<settings>
				<requestFieldName>storelocations_id</requestFieldName>
				<primaryFieldName>storelocations_id</primaryFieldName>
			</settings>
		</dataProvider>
	</dataSource>
	<fieldset name="general">
		<settings>
			<label>General</label>
		</settings>
		<field name="address" formElement="textarea" sortOrder="10">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">StoreLocations</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">Address</label>
				<dataScope>address</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">true</rule>
				</validation>
			</settings>
		</field>
		<field name="city" formElement="input" sortOrder="20">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">StoreLocations</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">City</label>
				<dataScope>city</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">true</rule>
				</validation>
			</settings>
		</field>
	    <field name="state">
	        <argument name="data" xsi:type="array">
	            <item name="options" xsi:type="object">Printq\StoreLocationsManagement\Model\Config\Source\State</item>
	            <item name="config" xsi:type="array">
	                <item name="label" xsi:type="string" translate="true">State</item>
	                <item name="formElement" xsi:type="string">select</item>
	                <item name="sortOrder" xsi:type="number">30</item>
	                <item name="source" xsi:type="string">StoreLocations</item>
	                <item name="customEntry" xsi:type="string">state</item>
	                <item name="dataScope" xsi:type="string">state</item>
	                <item name="validation" xsi:type="array">
	                    <item name="required-entry" xsi:type="boolean">true</item>
	                </item>
	            </item>
	        </argument>
	    </field>
		<field name="zip" formElement="input" sortOrder="40">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">StoreLocations</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">Zipcode</label>
				<dataScope>zip</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">true</rule>
				</validation>
			</settings>
		</field>
		<field name="phone" formElement="input" sortOrder="50">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">StoreLocations</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">Phone Number</label>
				<dataScope>phone</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">true</rule>
				</validation>
			</settings>
		</field>
		<field name="store_number" formElement="input" sortOrder="60">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">StoreLocations</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">Store Number</label>
				<dataScope>store_number</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">true</rule>
				</validation>
			</settings>
		</field>
		<field name="is_active" component="Magento_Ui/js/form/element/single-checkbox-toggle-notice" formElement="checkbox">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="default" xsi:type="number">0</item>
                </item>
            </argument>
            <settings>
                <dataType>boolean</dataType>
                <label translate="true">Is Active</label>
            </settings>
            <formElements>
                <checkbox>
                    <settings>
                        <valueMap>
                            <map name="false" xsi:type="number">0</map>
                            <map name="true" xsi:type="number">1</map>
                        </valueMap>
                        <prefer>toggle</prefer>
                    </settings>
                </checkbox>
            </formElements>
        </field>
		<field name="customer_group">
	        <argument name="data" xsi:type="array">
	            <item name="options" xsi:type="object">Magento\Customer\Model\ResourceModel\Group\Collection</item>
	            <item name="config" xsi:type="array">
	                <item name="label" xsi:type="string" translate="true">Customer Group</item>
	                <item name="formElement" xsi:type="string">select</item>
	                <item name="sortOrder" xsi:type="number">80</item>
	                <item name="source" xsi:type="string">StoreLocations</item>
	                <item name="customEntry" xsi:type="string">customer_group</item>
	                <item name="dataScope" xsi:type="string">customer_group</item>
	                <item name="validation" xsi:type="array">
	                    <item name="required-entry" xsi:type="boolean">true</item>
	                </item>
	            </item>
	        </argument>
	    </field>
        <field name="products">
            <argument name="data" xsi:type="array">
                <item name="options" xsi:type="object">Printq\StoreLocationsManagement\Model\Config\Source\Product</item>
                <item name="config" xsi:type="array">
                    <item name="dataType" xsi:type="string">text</item>
                    <item name="label" xsi:type="string" translate="true">Products</item>
                    <item name="componentType" xsi:type="string">field</item>
                    <item name="formElement" xsi:type="string">select</item>
                    <item name="component" xsi:type="string">Magento_Ui/js/form/element/ui-select</item>
                    <item name="elementTmpl" xsi:type="string">ui/grid/filters/elements/ui-select</item>
                    <item name="dataScope" xsi:type="string">products</item>
                    <item name="source" xsi:type="string">StoreLocations</item>
                    <item name="filterOptions" xsi:type="boolean">true</item>
                    <item name="showCheckbox" xsi:type="boolean">true</item>
                    <item name="disableLabel" xsi:type="boolean">true</item>
                    <item name="multiple" xsi:type="boolean">true</item>
                    <item name="levelsVisibility" xsi:type="number">1</item>
                    <item name="sortOrder" xsi:type="number">90</item>
                    <item name="required" xsi:type="boolean">true</item>
                    <item name="validation" xsi:type="array">
                        <item name="required-entry" xsi:type="boolean">true</item>
                    </item>
                    <item name="listens" xsi:type="array">
                        <item name="${ $.namespace }.${ $.namespace }:responseData" xsi:type="string">setParsed</item>
                    </item>
                </item>
            </argument>
        </field>
		<field name="frachise_owner" formElement="input" sortOrder="100">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">StoreLocations</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">Frachise Owner</label>
				<dataScope>frachise_owner</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">true</rule>
				</validation>
			</settings>
		</field>
		<field name="area_director" formElement="input" sortOrder="110">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">StoreLocations</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">Area Director</label>
				<dataScope>area_director</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">true</rule>
				</validation>
			</settings>
		</field>
		<field name="soft_drink">
            <argument name="data" xsi:type="array">
                <item name="options" xsi:type="object">Printq\StoreLocationsManagement\Model\Config\Source\SoftDrink</item>
                <item name="config" xsi:type="array">
                    <item name="dataType" xsi:type="string">text</item>
                    <item name="label" xsi:type="string" translate="true">Soft Drinks</item>
                    <item name="componentType" xsi:type="string">field</item>
                    <item name="formElement" xsi:type="string">select</item>
                    <item name="component" xsi:type="string">Magento_Ui/js/form/element/ui-select</item>
                    <item name="elementTmpl" xsi:type="string">ui/grid/filters/elements/ui-select</item>
                    <item name="dataScope" xsi:type="string">soft_drink</item>
                    <item name="source" xsi:type="string">StoreLocations</item>
                    <item name="filterOptions" xsi:type="boolean">true</item>
                    <item name="showCheckbox" xsi:type="boolean">true</item>
                    <item name="disableLabel" xsi:type="boolean">true</item>
                    <item name="multiple" xsi:type="boolean">true</item>
                    <item name="levelsVisibility" xsi:type="number">1</item>
                    <item name="sortOrder" xsi:type="number">120</item>
                    <item name="required" xsi:type="boolean">true</item>
                    <item name="validation" xsi:type="array">
                        <item name="required-entry" xsi:type="boolean">true</item>
                    </item>
                    <item name="listens" xsi:type="array">
                        <item name="${ $.namespace }.${ $.namespace }:responseData" xsi:type="string">setParsed</item>
                    </item>
                </item>
            </argument>
        </field>
	</fieldset>
</form>
